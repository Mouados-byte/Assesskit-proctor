<main class="bg-zinc-50 h-full">
  <div class="mx-auto max-w-7xl px-4 sm:px-6 lg:px-8">
    <div class="pt-8 flex items-center justify-between gap-x-4">
      <h1 class="text-2xl font-bold leading-7 text-gray-900 sm:truncate sm:text-3xl sm:tracking-tight">Tests</h1>
  
      <a href="/customer/assessments/new?step=choose_tests" class="inline-flex items-center gap-x-1.5 rounded-full bg-blue-600 px-4 py-2 text-center text-sm font-semibold text-white shadow-sm hover:bg-blue-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-blue-600">
        <%= svg_tag "plus-circle" %>
        Create assessment
      </a>
    </div>

    <div class="mt-8 grid grid-cols-1 gap-x-12 gap-y-8 lg:grid-cols-4">
      <!-- Filters -->
      <%= form_with url: tests_path, method: :get, data: { controller: "search", search_target: "form", "turbo-frame": "filter_section" } do |form| %>
        <div class="relative rounded-md-sm w-full">
          <%= text_field_tag :search_query,
                            params[:search_query],
                            placeholder: "Search tests",
                            class: "block w-full rounded-xl py-2.5 pr-5 pl-10 text-gray-800 border border-gray-800 placeholder:text-gray-400 font-medium focus:border focus:border-gray-800 focus:ring-1 focus:ring-inset focus:ring-gray-800 sm:text-sm sm:leading-6",
                            data: {
                              search_target: "input",
                              action: "input->search#search"
                            } %>
          <div class="pointer-events-none absolute inset-y-0 left-0 flex items-center pl-3">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2.5" stroke="currentColor" class="h-5 w-5 text-gray-400">
              <path stroke-linecap="round" stroke-linejoin="round" d="m21 21-5.197-5.197m0 0A7.5 7.5 0 1 0 5.196 5.196a7.5 7.5 0 0 0 10.607 10.607Z" />
            </svg>
          </div>
        </div>

        <h2 class="pt-7 text-xl font-bold text-black">Filters</h2>

        <div data-controller="filter" class="border-b border-gray-200 py-6">
          <h3 class="-my-3 flow-root">
            <button type="button" class="flex w-full items-center justify-between py-3 text-sm text-gray-600 hover:text-gray-700" data-action="filter#toggle" aria-controls="filter-section-0" aria-expanded="false">
              <span class="font-bold text-gray-900">Highlights</span>
              <span class="ml-6 flex items-center">
                <%= svg_tag "chevron-up", class: "lg:hidden size-4", "data-filter-target": "chevronDown", "stroke-width": 2.5 %>
                <%= svg_tag "chevron-down", class: "hidden lg:block size-4", "data-filter-target": "chevronUp", "stroke-width": 2.5 %>
              </span>
            </button>
          </h3>

          <div data-filter-target="section" class="hidden lg:block pt-6">
            <div class="space-y-4">
              <div class="flex items-center">
                <input id="highlight-1" name="highlights[]" value="free" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="highlight-1" class="ml-3 text-sm font-medium text-gray-800 flex gap-x-2 items-center">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" class="size-4">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M21 11.25v8.25a1.5 1.5 0 0 1-1.5 1.5H5.25a1.5 1.5 0 0 1-1.5-1.5v-8.25M12 4.875A2.625 2.625 0 1 0 9.375 7.5H12m0-2.625V7.5m0-2.625A2.625 2.625 0 1 1 14.625 7.5H12m0 0V21m-8.625-9.75h18c.621 0 1.125-.504 1.125-1.125v-1.5c0-.621-.504-1.125-1.125-1.125h-18c-.621 0-1.125.504-1.125 1.125v1.5c0 .621.504 1.125 1.125 1.125Z" />
                  </svg>
                  <span>Free <span class="text-gray-500">(60)</span></span>
                </label>
              </div>
              <div class="flex items-center">
                <input id="highlight-2" name="highlights[]" value="popular" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="highlight-2" class="ml-3 text-sm font-medium text-gray-800 flex gap-x-2 items-center">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" class="size-4">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M15.362 5.214A8.252 8.252 0 0 1 12 21 8.25 8.25 0 0 1 6.038 7.047 8.287 8.287 0 0 0 9 9.601a8.983 8.983 0 0 1 3.361-6.867 8.21 8.21 0 0 0 3 2.48Z" />
                  </svg>
                  <span>Popular <span class="text-gray-500">(10)</span></span>
                </label>
              </div>
              <div class="flex items-center">
                <input id="highlight-3" name="highlights[]" value="new" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="highlight-3" class="ml-3 text-sm font-medium text-gray-800 flex gap-x-2 items-center">
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" class="size-4">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M9.813 15.904 9 18.75l-.813-2.846a4.5 4.5 0 0 0-3.09-3.09L2.25 12l2.846-.813a4.5 4.5 0 0 0 3.09-3.09L9 5.25l.813 2.846a4.5 4.5 0 0 0 3.09 3.09L15.75 12l-2.846.813a4.5 4.5 0 0 0-3.09 3.09ZM18.259 8.715 18 9.75l-.259-1.035a3.375 3.375 0 0 0-2.455-2.456L14.25 6l1.036-.259a3.375 3.375 0 0 0 2.455-2.456L18 2.25l.259 1.035a3.375 3.375 0 0 0 2.456 2.456L21.75 6l-1.035.259a3.375 3.375 0 0 0-2.456 2.456ZM16.894 20.567 16.5 21.75l-.394-1.183a2.25 2.25 0 0 0-1.423-1.423L13.5 18.75l1.183-.394a2.25 2.25 0 0 0 1.423-1.423l.394-1.183.394 1.183a2.25 2.25 0 0 0 1.423 1.423l1.183.394-1.183.394a2.25 2.25 0 0 0-1.423 1.423Z" />
                  </svg>
                  <span>New <span class="text-gray-500">(123)</span></span>
                </label>
              </div>
            </div>
          </div>
        </div>

        <div data-controller="filter" class="border-b border-gray-200 py-6">
          <h3 class="-my-3 flow-root">
            <button type="button" class="flex w-full items-center justify-between py-3 text-sm text-gray-600 hover:text-gray-700" data-action="filter#toggle" aria-controls="filter-section-0" aria-expanded="false">
              <span class="font-bold text-gray-900">Test focus</span>
              <span class="ml-6 flex items-center">
                <%= svg_tag "chevron-up", class: "lg:hidden size-4", "data-filter-target": "chevronDown", "stroke-width": 2.5 %>
                <%= svg_tag "chevron-down", class: "hidden lg:block size-4", "data-filter-target": "chevronUp", "stroke-width": 2.5 %>
              </span>
            </button>
          </h3>

          <div data-filter-target="section" class="hidden lg:block pt-6">
            <div class="space-y-4">
              <% TestCategory.order(:title).each.with_index(1) do |test_type, index| %>
                <div class="flex items-center">
                  <%= check_box_tag 'test_category[]', 
                            test_type.id, 
                            params[:test_category]&.include?(test_type.id.to_s), 
                            id: "test-category-#{index}", 
                            class: "h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500",
                            data: { 
                              search_target: "checkbox",
                              action: "change->search#submitOnCheck"
                            } %>
                  <%= label_tag "test-category-#{index}", "#{test_type.title} <span class='text-gray-500'>(#{test_type.tests.count})</span>".html_safe, class: "ml-3 text-sm font-medium text-gray-800" %>
                </div>
              <% end %>
            </div>
          </div>
        </div>

        <div data-controller="filter" class="border-b border-gray-200 py-6">
          <h3 class="-my-3 flow-root">
            <button type="button" class="flex w-full items-center justify-between py-3 text-sm text-gray-600 hover:text-gray-700" data-action="filter#toggle" aria-controls="filter-section-0" aria-expanded="false">
              <span class="font-bold text-gray-900">Test format</span>
              <span class="ml-6 flex items-center">
                <%= svg_tag "chevron-up", class: "lg:hidden size-4", "data-filter-target": "chevronDown", "stroke-width": 2.5 %>
                <%= svg_tag "chevron-down", class: "hidden lg:block size-4", "data-filter-target": "chevronUp", "stroke-width": 2.5 %>
              </span>
            </button>
          </h3>

          <div data-filter-target="section" class="hidden lg:block pt-6">
            <div class="space-y-4">
              <% Test.types.each.with_index(1) do |type, index| %>
                <div class="flex items-center">
                  <%= check_box_tag 'test_type[]',
                                    type, 
                                    params[:test_type]&.include?(type), 
                                    id: "test-type-#{index}", 
                                    class: "h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500",
                                    data: { 
                                      search_target: "checkbox",
                                      action: "change->search#submitOnCheck"
                                    } %>
                  <%= label_tag "test-type-#{index}", "#{t(".types.#{type}")} <span class='text-gray-500'>(#{type.camelize.constantize.count})</span>".html_safe, class: "ml-3 text-sm font-medium text-gray-800" %>
                </div>
              <% end %>
            </div>
          </div>
        </div>

        <div data-controller="filter" class="border-b border-gray-200  py-6">
          <h3 class="-my-3 flow-root">
            <button type="button" class="flex w-full items-center justify-between py-3 text-sm text-gray-600 hover:text-gray-700" data-action="filter#toggle" aria-controls="filter-section-0" aria-expanded="false">
              <span class="font-bold text-gray-900">Test duration</span>
              <span class="ml-6 flex items-center">
                <%= svg_tag "chevron-up", class: "lg:hidden size-4", "data-filter-target": "chevronDown", "stroke-width": 2.5 %>
                <%= svg_tag "chevron-down", class: "hidden lg:block size-4", "data-filter-target": "chevronUp", "stroke-width": 2.5 %>
              </span>
            </button>
          </h3>

          <div data-filter-target="section" class="hidden lg:block pt-6">
            <div class="space-y-4">
              <div class="flex items-center">
                <input id="test-duration-1" name="test-duration[]" value="less_10" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="test-duration-1" class="ml-3 text-sm font-medium text-gray-800">Up to 10 mins <span class="text-gray-500">(363)</span></label>
              </div>
              <div class="flex items-center">
                <input id="test-duration-2" name="test-duration[]" value="11_to_20" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="test-duration-2" class="ml-3 text-sm font-medium text-gray-800">11-20 mins <span class="text-gray-500">(21)</span></label>
              </div>
              <div class="flex items-center">
                <input id="test-duration-3" name="test-duration[]" value="21_to_30" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="test-duration-3" class="ml-3 text-sm font-medium text-gray-800">21-30 mins <span class="text-gray-500">(19)</span></label>
              </div>
              <div class="flex items-center">
                <input id="test-duration-4" name="test-duration[]" value="31_to_60" type="checkbox" class="h-4 w-4 rounded border-gray-300 text-blue-600 focus:ring-blue-500">
                <label for="test-duration-4" class="ml-3 text-sm font-medium text-gray-800">31-60 mins <span class="text-gray-500">(13)</span></label>
              </div>
            </div>
          </div>
        </div>
        <div class="py-6 lg:pb-10">
          <%= link_to "Clear", tests_path, class: "black-button" %>
        </div>
      <% end %>

      <div class="lg:col-span-3">
        <%= turbo_frame_tag "filter_section" do %>
          <p class="text-base text-gray-700 font-bold">
            <% if params[:search_query].present? || params[:test_category].present? || params[:test_type].present? %>
              Results (<%= @tests.count %>)
            <% else %>
              <%= pluralize(@tests.count, 'test', 'tests') %> available.
            <% end %>
          </p>

          <div class="mt-3 pb-16">
            <% if @tests.empty? %>
              <div class="pt-20 pb-8">
                <div class="flex justify-center">
                  <svg xmlns="http://www.w3.org/2000/svg" width="160" height="125" viewBox="0 0 160 125" fill="none">
